<!DOCTYPE html>
<html>

<head>
    <title>splice function</title>
</head>
<script>
    let months=[2,3,4,5,6]
    console.log(months)

    //insert 1 month number in 0 index
    months.splice(6,0,7,8,9,10,11,12)
    console.log(months)

    //insert 7 to 12 month number after 6th month
    months.splice(6,0,7,8,9,10,11,12)
    console.log(months)

    //insert 91,92,93 month numbers after 9 element
    months.splice(10,0,91,92,93)
    console.log(months)

    //remove first 3 months numbers
    months.splice(0,3)
    console.log(months)

    //remove 91,92,93 month numbers
    months.splice(7,3)
    console.log(months)

    //add 1,2,3 month numbers in the begin,remove 4,5 month numbers
    months.splice(0,2,1,2,3)
    console.log(months)
    </script>

    <body>

    </body>

</html>

